extends layout

block content
    .header
        nav.nav-extended
            .nav-wrapper
               a.brand-logo(href='#') Image&nbsp;Compressor
            .nav-content
                ul.tabs.tabs-transparent
                    li.tab
                        a.active(href='#one-to-one') One to one
                    li.tab
                        a(href='#diff') Diff
                    li.tab
                        a(href='#histogram') Histogram
                    li.tab.disabled
                        a(href='#color-model') 3D RGB Color Model
    .content
        .drop-zone
            .center
                span.display-block DROP IMAGE HERE OR
                a.display-block.waves-effect.waves-light.blue.btn select from disk
                input#fake-button(type="file", accept=".jpg,.jpeg,.png")
        #tabs.hidden
            include tabs/one-to-one.pug
            include tabs/diff
            include tabs/histogram
            include tabs/3d-color-model